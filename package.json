  },
  "keywords": [
    "knockout",
    "spa"
  ],
  "author": "Zhao.Yinji",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "vorpal": "^1.12.0",
    "webpack": "^3.6.0"
  },
  "directoryName": {
    "dist": "dist",
    "repo": "ko-spa",
    "src": "src"
  },
  "fileName": {
    "bundle": "bundle.js"
  },
  "dependencies": {
    "html-loader": "^0.5.1",
