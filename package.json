{
  "name": "knockout-spa",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && xcopy \"src/index.html\" \"dist/\" /F /Y && xcopy \"src/style.css\" \"dist/\" /F /Y && webpack-dev-server --content-base src/ --inline --hot",
